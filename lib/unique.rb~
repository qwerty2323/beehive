require_relative 'CsvReader'
module Unique
  # Here will be documentation comment
  def self.unique_values_of(name)
    @@array ||= [] 

    CsvReader.harvest.each do |item|
      @@array    << item[name]
    end
    @@array.uniq!
  end
  
